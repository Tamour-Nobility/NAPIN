{
  "ConnectionStrings": {
    "npm": "Server=*****;Database=****;MultipleActiveResultSets=true;user id=****;password=*****",
    "era": "Server=*****;Database=*****;MultipleActiveResultSets=true;user id=*****;password=*****"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Delay": "3600000",
  "X12Parser": {
    "Schema": "dbo",
    "ContainerSchema": "dbo",
    "ThrowExceptionOnSyntaxErrors": "true",
    "IndexedSegments": "AMT, BHT, CLM, DMG, DSB, DTP, HI, HL, LIN, LQ, LX, MEA, N3, N4, NM1, NTE, PAT, PER, PRV, QTY, REF, SBR, SV1, SV2",
    "ParseSearchPattern": "*.*",
    "SqlSegmentBatchSize": "1000",
    "SqlDateType": "date",
    "Source": "remits",
    "Destination": "Documents/ERA"
    //"CSIClaimBatchPath": "Documents/ERA"
  }
}